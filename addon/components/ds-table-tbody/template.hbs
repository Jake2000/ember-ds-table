{{#if hasBlock}}
    {{yield (hash cell=(component 'ds-table-cell')) content}}
{{else}}
    {{#each content as |record|}}
        <tr>
            {{#each columns as |column|}}
                {{ds-table-cell
                    isVisible=column.isVisible
                    column=column
                    record=(get record column.path)
                }}
            {{/each}}
        </tr>
    {{/each}}
{{/if}}
