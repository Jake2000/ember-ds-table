{{#if hasBlock}}
    {{yield
    	(hash
    		thead=(component 'ds-table-thead'
    			messages=messages
    			columns=columns
    		)
    		tbody=(component 'ds-table-tbody'
    			messages=messages
    			columns=columns
    			content=model.content
    			loading=loading
    		)
    	)
        (hash
            countPages=countPages
            skip=skip
        )
    }}
{{else}}
    {{ds-table-thead
		messages=messages
		columns=columns
	}}
	{{ds-table-tbody
		messages=messages
		columns=columns
		content=model.content
		loading=loading
	}}
{{/if}}
