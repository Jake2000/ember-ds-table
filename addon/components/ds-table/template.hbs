{{#if hasBlock}}
    {{yield
    	(hash
    		thead=(component 'ds-table-thead'
    			messages=messages
    			columns=columns
    		)
    		tbody=(component 'ds-table-tbody'
    			messages=messages
    			columns=columns
    			content=model.content
    			loading=loading
    		)
    		loading=(component 'ds-table-loading'
    			messages=messages
    			loading=loading
    		)
    		empty=(component 'ds-table-empty'
    			messages=messages
    			count=count
    			loading=loading
    		)
			pagination=(component 'ds-table-pagination'
    			messages=messages
    			currentPage=currentPage
    			countPages=countPages
                limit=limit
                skip=skip
    		)
    	)
    }}
{{else}}
    {{ds-table-thead
		messages=messages
		columns=columns
	}}
	{{ds-table-tbody
		messages=messages
		columns=columns
		content=model.content
		loading=loading
	}}
	{{ds-table-loading
		messages=messages
		loading=loading
	}}
	{{ds-table-empty
		messages=messages
		count=count
		loading=loading
	}}
	{{ds-table-pagination
        messages=messages
        currentPage=currentPage
        countPages=countPages
        limit=limit
        skip=skip
	}}
{{/if}}
